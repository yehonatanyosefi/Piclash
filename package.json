{
	"name": "piclash",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"date-fns": "^2.30.0",
		"firebase": "^9.22.0",
		"npm-run-all": "^4.1.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-dotenv": "^0.1.3",
		"react-loading-skeleton": "^3.3.1",
		"react-redux": "^8.0.5",
		"react-router": "^6.11.1",
		"react-router-dom": "^6.11.1",
		"react-scripts": "5.0.1",
		"redux": "^4.2.1",
		"redux-thunk": "^2.4.2",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"build:css": "postcss src/styles/tailwind.css -o src/styles/app.css",
		"watch:css": "postcss src/styles/tailwind.css -o src/styles/app.css --watch",
		"react-scripts:start": "sleep 5 && react-scripts start",
		"start": "run-p watch:css start:react",
		"start:react": "react-dotenv && react-scripts start",
		"build": "run-s build:css && react-dotenv && react-scripts build",
		"serve": "react-dotenv && serve build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"autoprefixer": "^10.4.14",
		"postcss-cli": "^10.1.0",
		"prop-types": "^15.8.1",
		"tailwindcss": "^3.3.2"
	}
}
